var todoApp = angular.module('todoApp');
todoApp.factory('registrationService',  function($http, $location){

   var register = {};
   
   register.registerUser = function(user){
   
    $http({
	      method = "POST",
	      url= 'register',
	      data = user

         }).then (function (response) {
			response.path
		},
   })
});